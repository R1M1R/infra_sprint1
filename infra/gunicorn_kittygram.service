[Unit]
Description=gunicorn daemon 
After=network.target 

[Service]
User=yc-user
WorkingDirectory=/home/yc-user/infra_sprint1/backend/
WorkingDirectory=/home/yc-user/taski/backend/
ExecStart=/home/yc-user/infra_sprint1/venv/bin/gunicorn --bind 0.0.0.0:8000 kittygram_backend.wsgi
ExecStart=/home/yc-user/taski/venv/bin/gunicorn --bind 0.0.0.0:6000 taski_backend.wsgi
[Install]
WantedBy=multi-user.target 
